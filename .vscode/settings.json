{
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "eslint.lintTask.enable": true,
    "git.branchProtection": [
        "main",
        "development",
        "test",
        "release",
        "hotfix"
    ],
    "git.branchValidationRegex": "^(?:feature|enhance|bugfix|general)\/(?:[a-z0-9]+(?:-[a-z0-9]+)*){10,}$",
    "files.eol": "\r\n"
}